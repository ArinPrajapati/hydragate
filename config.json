{
    "jwt_secret": "my-super-secret-key-change-in-production",
    "forward_claims": {
        "sub": "X-User-Id",
        "role": "X-User-Role"
    },
    "api_keys": {
        "test-key-1": "dev-client",
        "test-key-2": "mobile-app"
    },
    "rate_limit": {
        "enabled": true,
        "capacity": 100,
        "refill_rate": 100
    },
    "routes": [
        {
            "route": "api",
            "target": "http://localhost:9000",
            "protected": true
        },
        {
            "route": "date",
            "target": "http://localhost:9000",
            "protected": false
        }
    ]
}